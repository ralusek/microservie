{
  "name": "microservie",
  "version": "0.3.0",
  "description": "Microservice interface and middleware.",
  "keywords": [
    "middleware",
    "microservice",
    "interface",
    "json",
    "jsonschema"
  ],
  "author": "Tomas Savigliano",
  "license": "MIT",
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "npx ttsc",
    "start": "npm run dev:start",
    "dev:start": "NODE_PATH=./src npx ts-node-dev ./src/index",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "prettify": "npx prettier --write '**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run build"
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "bluebird": "3.x"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "12.7.5",
    "@types/uuid": "3.4.5",
    "@types/validator": "10.11.3",
    "bluebird": "3.7.2",
    "chai": "4.2.0",
    "husky": "4.2.5",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "ttypescript": "^1.5.10",
    "typescript": "3.8.3",
    "typescript-transform-paths": "^1.1.14"
  }
}
